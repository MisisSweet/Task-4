@model List<string>
@{
    ViewData["Title"] = "Таблица";
}
<div class="text-center border-warning border">
    @if(ViewData["loginUser"] != null){
        <h1 class="display-4">@((ViewData["loginUser"] as Person ).FullName )</h1>
    }
</div>
<div class="border-warning border">
    <button type="button" class="btn btn-primary">Блокировать</button>
    <button type="button" class="btn btn-primary">
        <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-lock" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
            <path fill-rule="evenodd" d="M11.5 8h-7a1 1 0 0 0-1 1v5a1 1 0 0 0 1 1h7a1 1 0 0 0 1-1V9a1 1 0 0 0-1-1zm-7-1a2 2 0 0 0-2 2v5a2 2 0 0 0 2 2h7a2 2 0 0 0 2-2V9a2 2 0 0 0-2-2h-7zm0-3a3.5 3.5 0 1 1 7 0v3h-1V4a2.5 2.5 0 0 0-5 0v3h-1V4z" />
        </svg>
    </button>
    <button type="button" class="btn btn-primary">
        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-x-square-fill" viewBox="0 0 16 16">
            <path d="M2 0a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2H2zm3.354 4.646L8 7.293l2.646-2.647a.5.5 0 0 1 .708.708L8.707 8l2.647 2.646a.5.5 0 0 1-.708.708L8 8.707l-2.646 2.647a.5.5 0 0 1-.708-.708L7.293 8 4.646 5.354a.5.5 0 1 1 .708-.708z" />
        </svg>
    </button>
</div>
<div>
</div>
<div class="table-responsive">
    <table class="table">
        <thead class="thead-inverse">
            <tr>
                <th><input id="Checkbox1" type="checkbox" /></th>
                <th>#</th>
                <th>Фамилия</th>
                <th>Имя</th>
                <th>Email</th>
                <th>Дата регистрации</th>
                <th>Последняя дата входа</th>
                <th>Статус</th>
            </tr>
        </thead>
        <tbody>
            
            @foreach (Person person in (ViewData["person"] as List<Person>)){
            <tr>
                <th><input id="Checkbox1" type="checkbox" /></th>
                <th scope="row">@person.ID</th>
                <th scope="row">@person.Information.FirstName</th>
                <th scope="row">@person.Information.LastName</th>
                <th scope="row">@person.Information.Email</th>
                <th scope="row">@person.Information.DateRegistr.ToShortDateString()</th>
                <th scope="row">@person.Information.LastLogin.ToShortDateString()</th>
                <th scope="row">@person.StringStatus</th>
            </tr>
            }
        </tbody>
    </table>
</div>
<div>
    <button type="button" class="btn btn-primary">Выход</button>
</div>